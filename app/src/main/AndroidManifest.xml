<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gutting.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/Theme.Timedoctor"
        android:uiOptions="splitActionBarWhenNarrow">
        <activity
            android:name="com.example.gutting.myapplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Aktivierung der DataSafeActivity-->
        <activity android:name=".DataSafeActivity"/> // Nur aus Testzwecken
        <activity android:name=".AppListActivity"/>
        <activity android:name=".MainTabActivity"/>
        <activity android:name=".TopTenActivity"/>
    </application>

</manifest>
